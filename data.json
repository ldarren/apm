{
	"mod":{
		"qiqi": {
			"log": ["res", "output", "meta"],
			"call": ["req", "res", "opt"]
		},
		"util": {
			"validate": [["spec", "key"], ["req", "output"]],
			"extract": ["array", "key", "output"],
			"output": ["array", "key", "output"]
		},
		"session": {
			"signup": ["input"]
		}
	},
	"spec": {
		"session": [ "signup", "signin", "signout" ]
	},
	"routes": {
		"/v1/signup": [
			["qiqi.log", "_.res", "_.output", "_.meta"]
		],
		"/v1/signin": [
			["qiqi.log", "_.res", "_.output", "_.meta"]
		],
		"/v1/signout": [
			["qiqi.log", "_.res", "_.output", "_.meta"]
		]
	}
}
